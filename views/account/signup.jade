extends ../layout

block content
  .page-header
    h3 Signup

  form.form-horizontal(id='signup-form',method='POST')
    .form-group
      label.col-sm-3.control-label(for='email') Email
      .col-sm-7
        input.form-control(type='email', name='email', placeholder='Email', autofocus=true,value='jcwsw129@gmail.com')
    .form-group
      label.col-sm-3.control-label(for='password') Password
      .col-sm-7
        input.form-control(type='password',name='password', placeholder='Password',value='admin123')
    .form-group
      label.col-sm-3.control-label(for='confirmPassword') Confirm Password
      .col-sm-7
        input.form-control(type='password',name='confirmPassword',, placeholder='Confirm Password',value='admin123')
    .form-group
      .col-sm-7.col-sm-offset-3
        button.btn.btn-primary(type='submit') Signup

  
